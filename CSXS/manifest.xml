<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ExtensionBundleId="com.linkmanager2" ExtensionBundleVersion="1.0" Version="6.0">  <!-- MAJOR-VERSION-UPDATE-MARKER -->
  <ExtensionList>
    <Extension Id="com.linkmanager2.panel" Version="6.1.0"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="IDSN" Version="11.0"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="9.0"/>
      <!-- MAJOR-VERSION-UPDATE-MARKER -->
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.linkmanager2.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/dist/index.html</MainPath>
          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Link Manager 2</Menu>
          <Geometry>
            <Size>
              <Height>580</Height>
              <Width>1020</Width>
            </Size>
            <MaxSize>
              <Height>800</Height>
              <Width>1200</Width>
            </MaxSize>
            <MinSize>
              <Height>400</Height>
              <Width>600</Width>
            </MinSize>
          </Geometry>
          <Icons>
            <!-- <Icon Type="Normal">./host/icons/IconDark.png</Icon>
            <Icon Type="RollOver">./host/icons/IconDark.png</Icon>
            <Icon Type="DarkNormal">./host/icons/IconDark.png</Icon>
            <Icon Type="DarkRollOver">./host/icons/IconDark.png</Icon> -->
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
